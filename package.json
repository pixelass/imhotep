{
  "name": "imhotep",
  "version": "1.1.6",
  "description": "build tools",
  "keywords": [
    "ava",
    "babel",
    "browserslist",
    "esnext",
    "format",
    "lint",
    "nextjs",
    "polyfill",
    "prettier",
    "react",
    "stylelint",
    "test",
    "typescript"
  ],
  "license": "MIT",
  "author": "Gregor Adams <greg@pixelass.com> (http://pixelass.com)",
  "files": [
    "lib/*",
    "config/*"
  ],
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "bin": {
    "imhotep": "./lib/src/bin/bin.js"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "babel": "NODE_ENV=production babel src --out-dir lib/src --extensions '.ts,.tsx' --copy-files",
    "bundlesize": "bundlesize",
    "clean": "rimraf lib",
    "dev": "concurrently 'yarn babel --watch' 'yarn tsc --watch'",
    "format": "yarn run format:md; yarn run format:ts; yarn run format:js; yarn run format:json",
    "format:js": "prettier '{config,src,test}/**/*.js' --parser babylon --write",
    "format:json": "prettier *.json .babelrc .stylelintrc '{config,src,test}/**/*.json' --parser json --write",
    "format:md": "prettier *.md '{config,src,test}/**/*.md' --parser markdown --write",
    "format:ts": "prettier '{config,src,test}/**/*.{ts,tsx}' --parser typescript --write",
    "lint": "yarn run lint:ts",
    "lint:css": "stylelint '{demo,src}/**/*.{(pc|sc|c|le)ss,tsx?}'",
    "lint:ts": "tslint-config-prettier-check ./tslint.json",
    "package": "yarn clean && yarn babel",
    "refresh": "rimraf package && yarn package && yarn pack && tar -zxvf imhotep-v*.*.*.tgz && rimraf imhotep-v*.*.*.tgz",
    "prerelease": "yarn package; yarn test",
    "release": "standard-version --no-verify",
    "test": "ava",
    "types": "tsc --declaration --outDir lib --emitDeclarationOnly"
  },
  "dependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/chokidar": "^2.1.3",
    "@types/cosmiconfig": "^5.0.3",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/execa": "^0.9.0",
    "@types/globby": "^9.1.0",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.0.8",
    "@types/pify": "^3.0.2",
    "@types/prettier": "^1.16.4",
    "@types/stylelint": "^9.10.0",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "^3.1.6",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "bundlesize": "0.17.2",
    "chalk": "2.4.2",
    "chokidar": "3.0.1",
    "concurrently": "4.1.0",
    "cosmiconfig": "5.2.1",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "execa": "1.0.0",
    "file-loader": "4.0.0",
    "globby": "9.2.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.4.1",
    "meow": "5.0.0",
    "node-sass": "4.12.0",
    "pify": "4.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.18.2",
    "react-hot-loader": "4.11.1",
    "react-perf-devtool": "3.1.8",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "standard-version": "6.0.1",
    "style-loader": "0.23.1",
    "style-search": "0.1.0",
    "stylelint": "10.1.0",
    "stylelint-a11y": "1.2.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-declaration-block-no-ignored-properties": "2.1.0",
    "stylelint-high-performance-animation": "1.2.1",
    "stylelint-no-unsupported-browser-features": "3.0.2",
    "stylelint-order": "3.0.0",
    "ts-loader": "6.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.1",
    "webpack-merge": "4.2.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "ava": "^2.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "ava": "^2.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.10.0"
  }
}
