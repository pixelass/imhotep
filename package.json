{
  "name": "imhotep",
  "version": "1.1.5",
  "description": "build tools",
  "keywords": [
    "ava",
    "babel",
    "browserslist",
    "esnext",
    "format",
    "lint",
    "nextjs",
    "polyfill",
    "prettier",
    "react",
    "stylelint",
    "test",
    "typescript"
  ],
  "license": "MIT",
  "author": "Gregor Adams <greg@pixelass.com> (http://pixelass.com)",
  "files": [
    "lib/*",
    "config/*"
  ],
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "bin": {
    "imhotep": "./lib/src/bin/bin.js"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "babel": "NODE_ENV=production babel src --out-dir lib/src --extensions '.ts,.tsx' --copy-files",
    "bundlesize": "bundlesize",
    "clean": "rimraf lib",
    "dev": "concurrently 'yarn babel --watch' 'yarn tsc --watch'",
    "format": "yarn run format:md; yarn run format:ts; yarn run format:js; yarn run format:json",
    "format:js": "prettier '{config,src,test}/**/*.js' --parser babylon --write",
    "format:json": "prettier *.json .babelrc .stylelintrc '{config,src,test}/**/*.json' --parser json --write",
    "format:md": "prettier *.md '{config,src,test}/**/*.md' --parser markdown --write",
    "format:ts": "prettier '{config,src,test}/**/*.{ts,tsx}' --parser typescript --write",
    "lint": "yarn run lint:ts",
    "lint:css": "stylelint '{demo,src}/**/*.{(pc|sc|c|le)ss,tsx?}'",
    "lint:ts": "tslint-config-prettier-check ./tslint.json",
    "package": "yarn clean && yarn babel && yarn types",
    "refresh": "rimraf package && yarn package && yarn pack && tar -zxvf imhotep-v*.*.*.tgz && rimraf imhotep-v*.*.*.tgz",
    "prerelease": "yarn package; yarn test",
    "release": "standard-version --no-verify",
    "test": "ava",
    "types": "tsc --declaration --outDir lib --emitDeclarationOnly"
  },
  "dependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-decorators": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/chokidar": "1.7.5",
    "@types/cosmiconfig": "5.0.3",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/execa": "0.9.0",
    "@types/globby": "8.0.0",
    "@types/meow": "5.0.0",
    "@types/node": "10.12.18",
    "@types/pify": "3.0.2",
    "@types/prettier": "1.15.2",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/stylelint": "9.4.1",
    "@types/webpack": "4.4.22",
    "@types/webpack-dev-server": "3.1.1",
    "ava": "1.0.1",
    "babel-loader": "8.0.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "bundlesize": "0.17.0",
    "chalk": "2.4.2",
    "chokidar": "2.0.4",
    "concurrently": "4.1.0",
    "cosmiconfig": "5.0.7",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "execa": "1.0.0",
    "file-loader": "3.0.1",
    "globby": "8.0.1",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "meow": "5.0.0",
    "node-sass": "4.11.0",
    "pify": "4.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "prettier": "1.15.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-perf-devtool": "3.1.8",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "standard-version": "4.4.0",
    "style-loader": "0.23.1",
    "style-search": "0.1.0",
    "stylelint": "9.9.0",
    "stylelint-a11y": "1.1.9",
    "stylelint-color-format": "0.2.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-csstree-validator": "1.3.0",
    "stylelint-declaration-block-no-ignored-properties": "1.1.0",
    "stylelint-high-performance-animation": "1.1.1",
    "stylelint-no-unsupported-browser-features": "3.0.2",
    "stylelint-order": "2.0.0",
    "ts-loader": "5.3.3",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1",
    "winston": "3.1.0"
  },
  "peerDependencies": {
    "ava": "^1.0.0-rc.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "tslib": "1.9.3"
  }
}
